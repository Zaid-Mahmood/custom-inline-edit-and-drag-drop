{
  "version": 2,
  "builds": [
    {
      "src": "Server/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "db.json"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Server/server.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ]
}
